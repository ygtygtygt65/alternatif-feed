
# Semicolon-separated list of feed URLs (RSS/Atom XML or JSON like RSS.app)
FEED_URLS=https://rss.app/feeds/abcd.json;https://example.com/rss.xml

# Cache refresh interval in seconds (default 600 = 10min)
CACHE_TTL_SECONDS=600

# CORS allow origin. Use * to allow all (or your domain)
ALLOW_ORIGIN=*

# Optional: only include items whose title contains one of these (case-insensitive, use | as OR)
TITLE_FILTER=

# Optional: Basic auth (protect your endpoint)
BASIC_USER=
BASIC_PASS=
